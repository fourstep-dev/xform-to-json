<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:odk="http://www.opendatakit.org/xforms" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Trip Time Use</h:title>
    <model odk:xforms-version="1.0.0">
      <instance>
        <aLZkiU4mdvBP5dEyMQjmu6 id="aLZkiU4mdvBP5dEyMQjmu6">
          <start/>
          <end/>
          <social_media/>
          <duration_social_media/>
          <reading/>
          <duration_reading/>
          <checking_emails/>
          <duration_checking_emails/>
          <music/>
          <duration_music/>
          <play_game/>
          <duration_play_game/>
          <watching_movie/>
          <duration_watching_movie/>
          <learning_and_educational/>
          <duration_learning_and_educational/>
          <phone/>
          <duration_phone/>
          <talk_to_passengers/>
          <duration_talk_to_passengers/>
          <walking_dog/>
          <duration_walking_dog/>
          <exercising/>
          <duration_exercising/>
          <sleeping/>
          <duration_sleeping/>
          <no_activity/>
          <duration_no_activity/>
          <other/>
          <activity_other/>
          <duration_other/>
          <meta>
            <instanceID/>
          </meta>
        </aLZkiU4mdvBP5dEyMQjmu6>
      </instance>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/aLZkiU4mdvBP5dEyMQjmu6/start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/aLZkiU4mdvBP5dEyMQjmu6/end" type="dateTime"/>
      <bind nodeset="/aLZkiU4mdvBP5dEyMQjmu6/social_media" required="false()" type="string"/>
      <bind nodeset="/aLZkiU4mdvBP5dEyMQjmu6/duration_social_media" relevant=" /aLZkiU4mdvBP5dEyMQjmu6/social_media  != ''" required="true()" type="int"/>
      <bind nodeset="/aLZkiU4mdvBP5dEyMQjmu6/reading" required="false()" type="string"/>
      <bind nodeset="/aLZkiU4mdvBP5dEyMQjmu6/duration_reading" relevant=" /aLZkiU4mdvBP5dEyMQjmu6/reading  != ''" required="true()" type="int"/>
      <bind nodeset="/aLZkiU4mdvBP5dEyMQjmu6/checking_emails" required="false()" type="string"/>
      <bind nodeset="/aLZkiU4mdvBP5dEyMQjmu6/duration_checking_emails" relevant=" /aLZkiU4mdvBP5dEyMQjmu6/checking_emails  != ''" required="true()" type="int"/>
      <bind nodeset="/aLZkiU4mdvBP5dEyMQjmu6/music" required="false()" type="string"/>
      <bind nodeset="/aLZkiU4mdvBP5dEyMQjmu6/duration_music" relevant=" /aLZkiU4mdvBP5dEyMQjmu6/music  != ''" required="true()" type="int"/>
      <bind nodeset="/aLZkiU4mdvBP5dEyMQjmu6/play_game" required="false()" type="string"/>
      <bind nodeset="/aLZkiU4mdvBP5dEyMQjmu6/duration_play_game" relevant=" /aLZkiU4mdvBP5dEyMQjmu6/play_game  != ''" required="true()" type="int"/>
      <bind nodeset="/aLZkiU4mdvBP5dEyMQjmu6/watching_movie" required="false()" type="string"/>
      <bind nodeset="/aLZkiU4mdvBP5dEyMQjmu6/duration_watching_movie" relevant=" /aLZkiU4mdvBP5dEyMQjmu6/watching_movie  != ''" required="true()" type="int"/>
      <bind nodeset="/aLZkiU4mdvBP5dEyMQjmu6/learning_and_educational" required="false()" type="string"/>
      <bind nodeset="/aLZkiU4mdvBP5dEyMQjmu6/duration_learning_and_educational" relevant=" /aLZkiU4mdvBP5dEyMQjmu6/learning_and_educational  != ''" required="true()" type="int"/>
      <bind nodeset="/aLZkiU4mdvBP5dEyMQjmu6/phone" required="false()" type="string"/>
      <bind nodeset="/aLZkiU4mdvBP5dEyMQjmu6/duration_phone" relevant=" /aLZkiU4mdvBP5dEyMQjmu6/phone  != ''" required="true()" type="int"/>
      <bind nodeset="/aLZkiU4mdvBP5dEyMQjmu6/talk_to_passengers" required="false()" type="string"/>
      <bind nodeset="/aLZkiU4mdvBP5dEyMQjmu6/duration_talk_to_passengers" relevant=" /aLZkiU4mdvBP5dEyMQjmu6/talk_to_passengers  != ''" required="true()" type="int"/>
      <bind nodeset="/aLZkiU4mdvBP5dEyMQjmu6/walking_dog" required="false()" type="string"/>
      <bind nodeset="/aLZkiU4mdvBP5dEyMQjmu6/duration_walking_dog" relevant=" /aLZkiU4mdvBP5dEyMQjmu6/walking_dog  != ''" required="true()" type="int"/>
      <bind nodeset="/aLZkiU4mdvBP5dEyMQjmu6/exercising" required="false()" type="string"/>
      <bind nodeset="/aLZkiU4mdvBP5dEyMQjmu6/duration_exercising" relevant=" /aLZkiU4mdvBP5dEyMQjmu6/exercising  != ''" required="true()" type="int"/>
      <bind nodeset="/aLZkiU4mdvBP5dEyMQjmu6/sleeping" required="false()" type="string"/>
      <bind nodeset="/aLZkiU4mdvBP5dEyMQjmu6/duration_sleeping" relevant=" /aLZkiU4mdvBP5dEyMQjmu6/sleeping  != ''" required="true()" type="int"/>
      <bind nodeset="/aLZkiU4mdvBP5dEyMQjmu6/no_activity" required="false()" type="string"/>
      <bind nodeset="/aLZkiU4mdvBP5dEyMQjmu6/duration_no_activity" relevant=" /aLZkiU4mdvBP5dEyMQjmu6/no_activity  != ''" required="true()" type="int"/>
      <bind nodeset="/aLZkiU4mdvBP5dEyMQjmu6/other" required="false()" type="string"/>
      <bind nodeset="/aLZkiU4mdvBP5dEyMQjmu6/activity_other" relevant=" /aLZkiU4mdvBP5dEyMQjmu6/other  != ''" required="true()" type="string"/>
      <bind nodeset="/aLZkiU4mdvBP5dEyMQjmu6/duration_other" relevant=" /aLZkiU4mdvBP5dEyMQjmu6/other  != ''" required="true()" type="int"/>
      <bind jr:preload="uid" nodeset="/aLZkiU4mdvBP5dEyMQjmu6/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <trigger ref="/aLZkiU4mdvBP5dEyMQjmu6/social_media">
      <label>Checking social media</label>
    </trigger>
    <input ref="/aLZkiU4mdvBP5dEyMQjmu6/duration_social_media">
      <label>How long did it take? (In minutes)</label>
    </input>
    <trigger ref="/aLZkiU4mdvBP5dEyMQjmu6/reading">
      <label>Reading book for personal interest</label>
    </trigger>
    <input ref="/aLZkiU4mdvBP5dEyMQjmu6/duration_reading">
      <label>How long did it take? (In minutes)</label>
    </input>
    <trigger ref="/aLZkiU4mdvBP5dEyMQjmu6/checking_emails">
      <label>Checking emails related to work/University studies</label>
    </trigger>
    <input ref="/aLZkiU4mdvBP5dEyMQjmu6/duration_checking_emails">
      <label>How long did it take? (In minutes)</label>
    </input>
    <trigger ref="/aLZkiU4mdvBP5dEyMQjmu6/music">
      <label>Listening to music, podcast, etc.</label>
    </trigger>
    <input ref="/aLZkiU4mdvBP5dEyMQjmu6/duration_music">
      <label>How long did it take? (In minutes)</label>
    </input>
    <trigger ref="/aLZkiU4mdvBP5dEyMQjmu6/play_game">
      <label>Play video games</label>
    </trigger>
    <input ref="/aLZkiU4mdvBP5dEyMQjmu6/duration_play_game">
      <label>How long did it take? (In minutes)</label>
    </input>
    <trigger ref="/aLZkiU4mdvBP5dEyMQjmu6/watching_movie">
      <label>Watching movies, Netflix, YouTube, etc.</label>
    </trigger>
    <input ref="/aLZkiU4mdvBP5dEyMQjmu6/duration_watching_movie">
      <label>How long did it take? (In minutes)</label>
    </input>
    <trigger ref="/aLZkiU4mdvBP5dEyMQjmu6/learning_and_educational">
      <label>Learning and educational activities</label>
    </trigger>
    <input ref="/aLZkiU4mdvBP5dEyMQjmu6/duration_learning_and_educational">
      <label>How long did it take? (In minutes)</label>
    </input>
    <trigger ref="/aLZkiU4mdvBP5dEyMQjmu6/phone">
      <label>Talk to someone on the phone</label>
    </trigger>
    <input ref="/aLZkiU4mdvBP5dEyMQjmu6/duration_phone">
      <label>How long did it take? (In minutes)</label>
    </input>
    <trigger ref="/aLZkiU4mdvBP5dEyMQjmu6/talk_to_passengers">
      <label>Talk to other passengers</label>
    </trigger>
    <input ref="/aLZkiU4mdvBP5dEyMQjmu6/duration_talk_to_passengers">
      <label>How long did it take? (In minutes)</label>
    </input>
    <trigger ref="/aLZkiU4mdvBP5dEyMQjmu6/walking_dog">
      <label>Walking the dog</label>
    </trigger>
    <input ref="/aLZkiU4mdvBP5dEyMQjmu6/duration_walking_dog">
      <label>How long did it take? (In minutes)</label>
    </input>
    <trigger ref="/aLZkiU4mdvBP5dEyMQjmu6/exercising">
      <label>Exercising</label>
    </trigger>
    <input ref="/aLZkiU4mdvBP5dEyMQjmu6/duration_exercising">
      <label>How long did it take? (In minutes)</label>
    </input>
    <trigger ref="/aLZkiU4mdvBP5dEyMQjmu6/sleeping">
      <label>Sleeping</label>
    </trigger>
    <input ref="/aLZkiU4mdvBP5dEyMQjmu6/duration_sleeping">
      <label>How long did it take? (In minutes)</label>
    </input>
    <trigger ref="/aLZkiU4mdvBP5dEyMQjmu6/no_activity">
      <label>No activity, just siting and relaxing or thinking</label>
    </trigger>
    <input ref="/aLZkiU4mdvBP5dEyMQjmu6/duration_no_activity">
      <label>How long did it take? (In minutes)</label>
    </input>
    <trigger ref="/aLZkiU4mdvBP5dEyMQjmu6/other">
      <label>Other</label>
    </trigger>
    <input ref="/aLZkiU4mdvBP5dEyMQjmu6/activity_other">
      <label>Please indicate it below:</label>
    </input>
    <input ref="/aLZkiU4mdvBP5dEyMQjmu6/duration_other">
      <label>How long did it take? (In minutes)</label>
    </input>
  </h:body>
</h:html>
